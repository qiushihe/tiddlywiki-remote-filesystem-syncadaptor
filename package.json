{
  "name": "tiddlywiki-remote-filesystem-syncadaptor",
  "version": "0.0.1",
  "description": "A `syncadaptor` module for TiddlyWiki to synchronize tiddlers with remote filesystem (i.e. AWS S3).",
  "main": "src/index.tsx",
  "scripts": {
    "lint": "eslint --fix --ext .js --ext .ts ./src ./types ./babel",
    "build": "npm-run-all build:pre build:compile build:post",
    "build:pre": "shx rm -fr build",
    "build:compile": "babel src --copy-files --extensions \".js,.ts\" --out-dir build",
    "build:post": "shx rm -fr build/*.d.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:qiushihe/tiddlywiki-remote-filesystem-syncadaptor.git"
  },
  "keywords": [
    "tiddlywiki",
    "remote",
    "filesystem",
    "syncadaptor"
  ],
  "author": "Billy He",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/qiushihe/tiddlywiki-remote-filesystem-syncadaptor/pulls"
  },
  "homepage": "https://github.com/qiushihe/tiddlywiki-remote-filesystem-syncadaptor",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "shx": "^0.3.3"
  }
}
